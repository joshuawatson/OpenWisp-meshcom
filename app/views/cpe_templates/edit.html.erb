<h1>Editing cpe_template</h1>

<% form_for(@cpe_template) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :template %><br />
    <%= f.text_area :template %>
  </p>
  <p>
    <%= f.label :active %><br />
    <%= f.check_box :active %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @cpe_template %> |
<%= link_to 'Back', cpe_templates_path %>